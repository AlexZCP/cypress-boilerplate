{
  "baseUrl": "https://api.thevividtiger.com/cloudadmin/1.0/",
  "integrationFolder": "cypress/integration/cloudprinter-web-site/",
  "testFiles": "**/*.spec.js",

  "video": false,
  "screenshotOnRunFailure": false,
  
  "reporter": "cypress-multi-reporters",
   "reporterOptions": {
       "reporterEnabled": "mochawesome",
       "mochawesomeReporterOptions": {
           "reportDir": "cypress/reports/mocha",
           "quite": true,
           "overwrite": false,
           "html": true,
           "json": true
       }
   }
}

